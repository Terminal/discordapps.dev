	"dependencies": {
		"body-parser": "^1.18.3",
		"cookie-parser": "^1.4.3",
		"cors": "^2.8.5",
		"express": "^4.16.4",
		"express-handlebars": "^3.0.0",
		"express-session": "^1.15.6",
		"flat": "^4.1.0",
		"full-icu": "^1.2.1",
