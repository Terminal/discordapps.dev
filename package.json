	"devDependencies": {
		"eslint": "^4.19.1",
		"eslint-config-airbnb-base": "^11.3.2",
		"eslint-plugin-import": "^2.14.0"
	},
	"eslintConfig": {
		"extends": "airbnb-base",
		"plugins": [
			"import"
		],
		"rules": {
			"indent": [
				2
			],
			"no-tabs": 0,
			"comma-dangle": 0,
			"max-len": 0,
			"no-console": 0,
			"no-param-reassign": 0,
			"no-underscore-dangle": 0,
			"newline-per-chained-call": 0
		},
		"env": {
			"node": true,
			"browser": true
		}
	}
