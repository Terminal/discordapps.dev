{
	"name": "ls.terminal.ink-frontend",
	"version": "0.2.0",
	"author" : "Moon <me@luna.fyi>",
	"private": true,
	"scripts": {
	  "clean": "rimraf dist/",
	  "build": "yarn check && webpack -p --progress --display-optimization-bailout --env=prod",
	  "build-silent": "yarn check && webpack -p --env=prod",
	  "build-force": "yarn check; webpack -p --progress --display-optimization-bailout --env=prod",
	  "dev": "yarn && webpack-dev-server --progress --env=dev",
	  "dev-silent": "yarn && webpack-dev-server --env=dev",
	  "dev-dashboard": "yarn && webpack-dashboard --color blue -- webpack-dev-server --env=dev",
	  "test": "jest --watch",
	  "test-once": "jest",
	  "test-coverage": "jest --coverage",
	  "lint": "eslint src"
	},
	"jest": {
	  "snapshotSerializers": [
		"enzyme-to-json/serializer"
	  ],
	  "setupFiles": [
		"<rootDir>/jest.setup.js",
		"<rootDir>/jest.js"
	  ],
	  "moduleNameMapper": {
		"\\.(css|less|scss|styl|sss)$": "<rootDir>/jest.js"
	  }
	},
	"devDependencies": {
	  "ajv": "~5.3.0",
	  "babel-core": "~6.26.0",
	  "babel-eslint": "~8.0.0",
	  "babel-jest": "~21.2.0",
	  "babel-loader": "~7.1.0",
	  "babel-plugin-transform-class-properties": "^6.24.1",
	  "babel-plugin-transform-decorators-legacy": "~1.3.0",
	  "babel-polyfill": "~6.26.0",
	  "babel-preset-env": "~1.6.0",
	  "babel-preset-react": "~6.24.0",
	  "css-loader": "0.28.7",
	  "enzyme": "~3.1.0",
	  "enzyme-adapter-react-16": "~1.0.0",
	  "enzyme-to-json": "~3.1.0",
	  "eslint": "~4.10.0",
	  "eslint-plugin-babel": "~4.1.0",
	  "eslint-plugin-react": "~7.4.0",
	  "html-webpack-plugin": "~2.30.0",
	  "jest": "~21.2.0",
	  "node-sass": "~4.5.0",
	  "prop-types": "~15.6.0",
	  "react-hot-loader": "~3.1.0",
	  "react-test-renderer": "~16.0.0",
	  "rimraf": "~2.6.0",
	  "sass-loader": "~6.0.0",
	  "style-loader": "0.19.0",
	  "webpack": "~3.8.0",
	  "webpack-dashboard": "~1.0.0",
	  "webpack-dev-server": "~2.9.0",
	  "webpack-merge": "~4.1.0"
	},
	"dependencies": {
	  "file-loader": "~1.1.0",
	  "isomorphic-fetch": "~2.2.0",
	  "normalize.css": "~7.0.0",
	  "react": "~16.0.0",
	  "react-dom": "~16.0.0",
	  "react-intl": "~2.4.0",
	  "react-intl-redux": "0.7.0",
	  "react-redux": "~5.0.0",
	  "react-router-dom": "~4.2.0",
	  "react-router-redux": "5.0.0-alpha.8",
	  "redux": "~3.7.0",
	  "redux-saga": "0.16.0"
	}
  }